<?xml version="1.0" encoding="UTF-8"?>
<project basedir="../" default="run" name="nfl">
  <!-- Import our build properties -->   
    <property name="debuglevel" value="source,lines,vars"/>
    <property name="target" value="1.5"/>
    <property name="source" value="1.5"/>
    <path id="nfl.classpath">
	<pathelement location="classes"/>
		<pathelement location="../lib/htmllexer.jar"/>
	<pathelement location="../lib/htmlparser.jar"/>
	<pathelement location="../lib/mail.jar"/>
	<pathelement location="../pensieve/WEB-INF/classes"/>
	<pathelement location="../lib/ibatis-2.3.4.726.jar"/>
	<pathelement location="../lib/postgresql-8.3-604.jdbc4.jar"/>
	<pathelement location="${ant.home}/lib/xercesImpl.jar"/>
	
    </path>
    
	<target name="run">
		 <java classpathref="nfl.classpath" 
		 	classname="joejava.nfl.action.GetNFLUpdate" />
	</target>
</project>
